{
  "schemaVersion": 1,
  "id": "skythm",
  "version": "${version}",

  "name": "Skythm",
  "description": "A vanilla style SkyBlock Mod",
  "authors": [],
  "contact": {},

  "license": "MIT",
  "icon": "assets/skythm/icon.png",

  "environment": "*",
  "entrypoints": {
    "fabric-datagen": ["com.yipkei.skythm.client.SkythmDataGenerator"],
    "client": ["com.yipkei.skythm.client.SkythmClient"],
    "main": [
      "com.yipkei.skythm.Skythm"
    ]
  },

  "mixins": [
    "skythm.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}",
    "carpet" : "*",
    "carpetskyadditions": "*"
  }
}
